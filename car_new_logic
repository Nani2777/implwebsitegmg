    <script type="text/javascript">
        //<!-- 
        var selector = parent._taq;
        var flag = selector.is_Mobile;
        var device = "web";
        if (flag) {
            device = "mobile";
        }
        var option
        var number
        var page = parent.location.href;
        jQuery(".mandatory-message").css("display", "none");
        jQuery(".slide_2").css("display", "none");
        jQuery(".slide_3").css("display", "none");
        jQuery(".slide_2").animate({
            "opacity": "0"
        });
        jQuery(".slide_3").animate({
            "opacity": "0"
        });


        jQuery(".submitOccassionButton").click(function () {
            var count = 0;
            $.each($("input[name=preference]"), function (item, e) {

                if ($(e)[0].checked) {
                    if ($(e)[0].value === 'thankyou') {
                        jQuery(".slide_1").css("display", "none");
                        jQuery(".slide_2").css("display", "none");
                        jQuery(".slide_3").animate({
                            "opacity": "1"
                        });
                        option = $("input[name='preference']:checked").parent().find('.preferenceText')
                            .text();
                        console.log(option);
                        selector.events.push(["non_buying_reason_form1", {
                            "reason": option,
                            "device": device,
                            "page": page
                        }]);
                        jQuery(".slide_3").css("display", "block");
                        return false;
                    }
                    count++;
                }
            });
            if (count === 0) {
                jQuery(".mandatory-message").css("display", "block");
                return false;
            } else {
                jQuery(".slide_1").css("display", "none");
                jQuery(".slide_2").animate({
                    "opacity": "1"
                });
                jQuery(".slide_2").css("display", "block");
                jQuery(".mandatory-message").css("display", "none");
                return false;
            }
        });

        jQuery(".submitMobileNumberButton").click(function () {
            var number = $("#mobileNumber").val();

            if ((/^(?:(?:\+|0{0,2})91(\s*[\-]\s*)?|[0]?)?[6789]\d{9}$/).test(number)) {
                jQuery(".slide_2").css("display", "none");
                jQuery(".slide_3").animate({
                    "opacity": "1"
                });
                selector.events.push(["non_buying_reason", {
                    "reason": option,
                    "mobile": number,
                    "device": device,
                    "page": page
                }]);
                selector.property.push(["mobile",number]);
                jQuery(".slide_3").css("display", "block");
            } else {
                jQuery(".mandatory-message").css("display", "block");
            }
            return false;
        });
        //-->
    </script>
</body>

</html>